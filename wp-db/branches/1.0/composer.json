{
  "name": "pollen-solutions/wp-db",
    "description": "Pollen Wordpress Db Component - Wordpress adapter for the Pollen Database Component.",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-db/",
  "keywords": [
    "pollen-solutions",
    "component",
    "database",
    "eloquent"
  ],
  "time": "2021-04-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/database": "^1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpDb\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpDb\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container": "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
